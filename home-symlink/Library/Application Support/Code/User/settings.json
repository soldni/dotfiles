{
  "breadcrumbs.enabled": true,
  "cursorpyright.analysis.autoImportCompletions": true,
  "cursorpyright.analysis.typeCheckingMode": "basic",
  "diffEditor.maxComputationTime": 0,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontFamily": "Iosevka Medium",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.fontVariations": true,
  "editor.fontWeight": "normal",
  "editor.formatOnPaste": true,
  "editor.inlineSuggest.enabled": true,
  "editor.largeFileOptimizations": true,
  "editor.lineHeight": 1.3,
  "editor.minimap.autohide": "mouseover",
  "editor.minimap.enabled": false,
  "editor.multiCursorLimit": 100000,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.overviewRulerBorder": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [
    115
  ],
  "editor.selectionHighlight": false,
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 115,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.incrementalNaming": "smart",
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "git.replaceTagsWhenPull": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "latex-workshop.view.pdf.viewer": "tab",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "notebook.output.textLineLimit": 100,
  "terminal.external.osxExec": "Ghostty.app",
  "terminal.integrated.fontFamily": "Iosevka Medium",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.lineHeight": 1.1,
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.tabs.separator": " · ",
  "terminal.integrated.tabs.title": "${process}${separator}${cwd}",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "window.autoDetectColorScheme": true,
  "window.closeWhenEmpty": true,
  "window.openFilesInNewWindow": "on",
  "window.systemColorTheme": "auto",
  "window.title": "${rootName}${separator}${activeEditorMedium}",
  "window.titleSeparator": " · ",
  "workbench.colorTheme": "Monokai Pro Light (Filter Sun)",
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "workbench.fontAliasing": "auto",
  "workbench.iconTheme": "Monokai Pro Light (Filter Sun) Icons",
  "workbench.preferredDarkColorTheme": "Monokai Pro (Filter Spectrum)",
  "workbench.preferredHighContrastColorTheme": "Monokai Pro (Filter Spectrum)",
  "workbench.preferredHighContrastLightColorTheme": "Monokai Pro Light (Filter Sun)",
  "workbench.preferredLightColorTheme": "Monokai Pro Light (Filter Sun)",
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 16,
  "peacock.affectActivityBar": false,
  "peacock.affectTitleBar": false,
  "redhat.telemetry.enabled": false,
  "claudeCode.preferredLocation": "sidebar",
  "cursor.cpp.disabledLanguages": [],
  "claudeCode.initialPermissionMode": "acceptEdits",
  "claudeCode.selectedModel": "opus",
  "workbench.layoutControl.type": "menu",
  "remote.autoForwardPortsSource": "hybrid",
  "workbench.layoutControl.enabled": false,
  "remote.SSH.remotePlatform": {
    "spark": "linux"
  },
  "workbench.sideBar.location": "left",
  "window.newWindowProfile": "Default",
  "terminal.integrated.fontLigatures.enabled": true,
  "window.doubleClickIconToClose": true,
  "window.menuStyle": "inherit",
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "never",
  "workbench.activityBar.location": "hidden",
  "explorer.fileNesting.enabled": true,
  "workbench.startupEditor": "none",
}
